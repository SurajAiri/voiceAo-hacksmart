#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tejas/ai-voice-platform/apps/ai-agent/node_modules/.pnpm/wavefile@11.0.0/node_modules/wavefile/bin/node_modules:/home/tejas/ai-voice-platform/apps/ai-agent/node_modules/.pnpm/wavefile@11.0.0/node_modules/wavefile/node_modules:/home/tejas/ai-voice-platform/apps/ai-agent/node_modules/.pnpm/wavefile@11.0.0/node_modules:/home/tejas/ai-voice-platform/apps/ai-agent/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tejas/ai-voice-platform/apps/ai-agent/node_modules/.pnpm/wavefile@11.0.0/node_modules/wavefile/bin/node_modules:/home/tejas/ai-voice-platform/apps/ai-agent/node_modules/.pnpm/wavefile@11.0.0/node_modules/wavefile/node_modules:/home/tejas/ai-voice-platform/apps/ai-agent/node_modules/.pnpm/wavefile@11.0.0/node_modules:/home/tejas/ai-voice-platform/apps/ai-agent/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/wavefile@11.0.0/node_modules/wavefile/bin/wavefile.js" "$@"
else
  exec node  "$basedir/../.pnpm/wavefile@11.0.0/node_modules/wavefile/bin/wavefile.js" "$@"
fi
