#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tejas/ai-voice-platform/apps/public-widget/node_modules/.pnpm/sdp-transform@2.15.0/node_modules/sdp-transform/node_modules:/home/tejas/ai-voice-platform/apps/public-widget/node_modules/.pnpm/sdp-transform@2.15.0/node_modules:/home/tejas/ai-voice-platform/apps/public-widget/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tejas/ai-voice-platform/apps/public-widget/node_modules/.pnpm/sdp-transform@2.15.0/node_modules/sdp-transform/node_modules:/home/tejas/ai-voice-platform/apps/public-widget/node_modules/.pnpm/sdp-transform@2.15.0/node_modules:/home/tejas/ai-voice-platform/apps/public-widget/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sdp-transform/checker.js" "$@"
else
  exec node  "$basedir/../sdp-transform/checker.js" "$@"
fi
