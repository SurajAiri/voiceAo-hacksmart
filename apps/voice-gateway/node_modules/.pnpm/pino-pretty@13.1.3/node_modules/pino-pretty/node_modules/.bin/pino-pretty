#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tejas/ai-voice-platform/apps/voice-gateway/node_modules/.pnpm/pino-pretty@13.1.3/node_modules/pino-pretty/node_modules:/home/tejas/ai-voice-platform/apps/voice-gateway/node_modules/.pnpm/pino-pretty@13.1.3/node_modules:/home/tejas/ai-voice-platform/apps/voice-gateway/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tejas/ai-voice-platform/apps/voice-gateway/node_modules/.pnpm/pino-pretty@13.1.3/node_modules/pino-pretty/node_modules:/home/tejas/ai-voice-platform/apps/voice-gateway/node_modules/.pnpm/pino-pretty@13.1.3/node_modules:/home/tejas/ai-voice-platform/apps/voice-gateway/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
