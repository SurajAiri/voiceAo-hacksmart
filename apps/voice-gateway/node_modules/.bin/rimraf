#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tejas/ai-voice-platform/apps/voice-gateway/node_modules/.pnpm/rimraf@6.1.2/node_modules/rimraf/dist/esm/node_modules:/home/tejas/ai-voice-platform/apps/voice-gateway/node_modules/.pnpm/rimraf@6.1.2/node_modules/rimraf/dist/node_modules:/home/tejas/ai-voice-platform/apps/voice-gateway/node_modules/.pnpm/rimraf@6.1.2/node_modules/rimraf/node_modules:/home/tejas/ai-voice-platform/apps/voice-gateway/node_modules/.pnpm/rimraf@6.1.2/node_modules:/home/tejas/ai-voice-platform/apps/voice-gateway/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tejas/ai-voice-platform/apps/voice-gateway/node_modules/.pnpm/rimraf@6.1.2/node_modules/rimraf/dist/esm/node_modules:/home/tejas/ai-voice-platform/apps/voice-gateway/node_modules/.pnpm/rimraf@6.1.2/node_modules/rimraf/dist/node_modules:/home/tejas/ai-voice-platform/apps/voice-gateway/node_modules/.pnpm/rimraf@6.1.2/node_modules/rimraf/node_modules:/home/tejas/ai-voice-platform/apps/voice-gateway/node_modules/.pnpm/rimraf@6.1.2/node_modules:/home/tejas/ai-voice-platform/apps/voice-gateway/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/rimraf@6.1.2/node_modules/rimraf/dist/esm/bin.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/rimraf@6.1.2/node_modules/rimraf/dist/esm/bin.mjs" "$@"
fi
